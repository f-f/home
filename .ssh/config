Include local/*

Host *
  ServerAliveInterval 30
  AddKeysToAgent yes
  IgnoreUnknown UseKeychain
  UseKeychain yes
  AddressFamily inet
  IdentitiesOnly yes
  IdentityAgent /Users/fabrizio/.cache/prezto/ssh-agent.sock

Host claudius
  Hostname claudius
  User fabrizio
  Port 10000
  IdentityFile ~/.ssh/claudius
  DynamicForward 5001

Host tiberius
  Hostname tiberius
  Port 22
  User fabrizio
  IdentityFile ~/.ssh/tiberius

Host nero
  User fabrizio
  IdentityFile ~/.ssh/nero
  Hostname nero
  Port 22
  ForwardAgent yes

Host mamma
  User mariateresa
  Port 22
  Hostname mbp-di-maria

Host domitian
  Hostname domitian
  User fabrizio
  Port 10000
  IdentityFile ~/.ssh/claudius
  DynamicForward 5002

Host nerva
  Hostname nerva.ferrai.io
  User fabrizio
  Port 10000
  IdentityFile ~/.ssh/nerva
  DynamicForward 5000

Host remarkable
  User root
  IdentityFile ~/.ssh/remarkable
  Hostname 192.168.8.132
  Port 22
  PubkeyAcceptedKeyTypes +ssh-rsa
  HostKeyAlgorithms +ssh-rsa

Host github.com
  IdentityFile ~/.ssh/github


Host gitlab.com
  IdentityFile ~/.ssh/gitlabcom
