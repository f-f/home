Include local/*

Host *
  ServerAliveInterval 30
  AddKeysToAgent yes
  IgnoreUnknown UseKeychain
  UseKeychain yes
  AddressFamily inet
  IdentitiesOnly yes
  IdentityAgent /Users/fabrizio/.cache/prezto/ssh-agent.sock

Match host augustus exec "are_we_at_home"
  Hostname 192.168.8.102
  User fabrizio
  Port 10000
  IdentityFile ~/.ssh/augustus
  ForwardX11 yes
  DynamicForward 5000
Host augustus
  Hostname functional.pizza
  User fabrizio
  Port 10001
  IdentityFile ~/.ssh/augustus
  ForwardX11 yes
  DynamicForward 5000

Match host claudius exec "are_we_at_home"
  Hostname claudius.local
  User fabrizio
  Port 10000
  IdentityFile ~/.ssh/claudius
  DynamicForward 5001
Host claudius
  Hostname functional.pizza
  User fabrizio
  Port 10002
  IdentityFile ~/.ssh/claudius
  DynamicForward 5001

Host tiberius
  Hostname tiberius
  Port 22
  User fabrizio
  IdentityFile ~/.ssh/tiberius

Host nero
  User fabrizio
  IdentityFile ~/.ssh/nero
  Hostname nero
  Port 22
  ForwardAgent yes

Host mamma
  User mariateresa
  Port 22
  Hostname mbp-di-maria

Host nerva
  Hostname nerva.ferrai.io
  User fabrizio
  Port 10000
  IdentityFile ~/.ssh/nerva
  DynamicForward 5000

Host remarkable
  User root
  IdentityFile ~/.ssh/remarkable
  Hostname 192.168.8.132
  Port 22
  PubkeyAcceptedKeyTypes +ssh-rsa
  HostKeyAlgorithms +ssh-rsa

Host github.com
  IdentityFile ~/.ssh/github
  IdentityFile ~/.ssh/github-id3as

Host gitlab.com
  IdentityFile ~/.ssh/gitlabcom
