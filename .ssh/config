Include local/*

Host *
  ServerAliveInterval 30
  AddKeysToAgent yes
  IgnoreUnknown UseKeychain
  UseKeychain yes
  AddressFamily inet
  IdentitiesOnly yes

Host domitian
  Hostname domitian.ferrai.io
  User fabrizio
  Port 10000
  IdentityFile ~/.ssh/domitian

Host claudius
  Hostname 192.168.8.159
  User fabrizio
  Port 10000
  IdentityFile ~/.ssh/claudius

Host nerva
  Hostname nerva.ferrai.io
  User fabrizio
  Port 10000
  IdentityFile ~/.ssh/nerva
  DynamicForward 5000

Host augustus
  Hostname nilrecurring.duckdns.org
  User fabrizio
  Port 10001
  IdentityFile ~/.ssh/augustus
  ForwardX11 yes
  DynamicForward 5000

Host augustus-local
  Hostname 192.168.8.102
  User fabrizio
  Port 10000
  IdentityFile ~/.ssh/augustus
  ForwardX11 yes
  DynamicForward 5000

Host nero
  User nero
  IdentityFile ~/.ssh/github
  ProxyJump claudius
  Hostname 192.168.122.204
  Port 22

Host remarkable
  User root
  IdentityFile ~/.ssh/remarkable
  Hostname 192.168.8.132
  Port 22

Host github.com
  IdentityFile ~/.ssh/github

Host gitlab.com
  IdentityFile ~/.ssh/gitlabcom

