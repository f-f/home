Include local/*

Host *
  ServerAliveInterval 30
  AddKeysToAgent yes
  IgnoreUnknown UseKeychain
  UseKeychain yes
  AddressFamily inet
  IdentitiesOnly yes

Match host augustus exec "are_we_at_home"
  Hostname 192.168.8.102
  User fabrizio
  Port 10000
  IdentityFile ~/.ssh/augustus
  ForwardX11 yes
  DynamicForward 5000
Host augustus
  Hostname nilrecurring.duckdns.org
  User fabrizio
  Port 10001
  IdentityFile ~/.ssh/augustus
  ForwardX11 yes
  DynamicForward 5000

Match host claudius exec "are_we_at_home"
  Hostname 192.168.8.159
  User fabrizio
  Port 10000
  IdentityFile ~/.ssh/claudius
  DynamicForward 5001
Host claudius
  Hostname functional.pizza
  User fabrizio
  Port 10002
  IdentityFile ~/.ssh/claudius
  DynamicForward 5001

Match host tiberius exec "are_we_at_home"
  Hostname tiberius.local
  User fabrizio
  Port 22
  IdentityFile ~/.ssh/tiberius
Host tiberius
  Hostname functional.pizza
  User fabrizio
  Port 10003
  IdentityFile ~/.ssh/tiberius

Host nero
  User fabrizio
  IdentityFile ~/.ssh/nero
  ProxyJump claudius
  Hostname 192.168.122.152
  Port 22

Host nerva
  Hostname nerva.ferrai.io
  User fabrizio
  Port 10000
  IdentityFile ~/.ssh/nerva
  DynamicForward 5000

Host remarkable
  User root
  IdentityFile ~/.ssh/remarkable
  Hostname 192.168.8.132
  Port 22
  IdentityAgent /Users/fabrizio/.cache/prezto/ssh-agent.sock
  PubkeyAcceptedKeyTypes +ssh-rsa
  HostKeyAlgorithms +ssh-rsa

Host github.com
  IdentityFile ~/.ssh/github
  IdentityFile ~/.ssh/github-id3as

Host gitlab.com
  IdentityFile ~/.ssh/gitlabcom
